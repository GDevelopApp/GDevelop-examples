                  },
                  "parameters": [
                    "",
                    "SamplePoint",
                    "NaiveCircleTarget.CenterX() + NaiveCircleTarget.Variable(LatestRandomDistance) * cos(NaiveCircleTarget.Variable(LatestRandomAngle))",
